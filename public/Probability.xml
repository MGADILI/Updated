<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:prob="http://www.example.org/probability#">

    <!-- Declaration of Classes -->
    <rdf:Description rdf:about="prob:ProbabilityTheory"/>
    <rdf:Description rdf:about="prob:ProbabilitySpace"/>
    <rdf:Description rdf:about="prob:SampleSpace"/>
    <rdf:Description rdf:about="prob:Event"/>
    <rdf:Description rdf:about="prob:ProbabilityMeasure"/>
    <rdf:Description rdf:about="prob:Distribution"/>
    <rdf:Description rdf:about="prob:DiscreteDistribution"/>
    <rdf:Description rdf:about="prob:ContinuousDistribution"/>
    <rdf:Description rdf:about="prob:Outcome"/>
    <rdf:Description rdf:about="prob:RandomVariable"/>
    <rdf:Description rdf:about="prob:DiscreteRandomVariable"/>
    <rdf:Description rdf:about="prob:Interpretation"/>
    <rdf:Description rdf:about="prob:FrequentistInterpretation"/>
    <rdf:Description rdf:about="prob:BayesianInterpretation"/>
    <rdf:Description rdf:about="prob:PropensityInterpretation"/>
    <rdf:Description rdf:about="prob:ConditionalProbability"/>
    <rdf:Description rdf:about="prob:Independence"/>

    <!-- Hierarchical Relationships (Subclasses and Properties) -->
    <rdf:Description rdf:about="prob:ProbabilitySpace">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilityTheory"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:SampleSpace">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilitySpace"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:Event">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilitySpace"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:ProbabilityMeasure">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilitySpace"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:Distribution">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilitySpace"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:DiscreteDistribution">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:Distribution"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:ContinuousDistribution">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:Distribution"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:Outcome">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:SampleSpace"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:RandomVariable">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilityTheory"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:DiscreteRandomVariable">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:RandomVariable"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:Interpretation">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilityTheory"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:FrequentistInterpretation">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:Interpretation"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:BayesianInterpretation">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:Interpretation"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:PropensityInterpretation">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:Interpretation"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:ConditionalProbability">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilityTheory"/>
    </rdf:Description>

    <rdf:Description rdf:about="prob:Independence">
        <rdf:type rdf:resource="rdfs:Class"/>
        <rdfs:subClassOf rdf:resource="prob:ProbabilityTheory"/>
    </rdf:Description>

    <!-- Properties (e.g., relationships between concepts) -->
    <!-- Example Property: HasOutcome -->
    <rdf:Description rdf:about="prob:hasOutcome">
        <rdf:type rdf:resource="rdf:Property"/>
        <rdfs:domain rdf:resource="prob:Event"/>
        <rdfs:range rdf:resource="prob:Outcome"/>
    </rdf:Description>

    <!-- Example Property: HasProbability -->
    <rdf:Description rdf:about="prob:hasProbability">
        <rdf:type rdf:resource="rdf:Property"/>
        <rdfs:domain rdf:resource="prob:ProbabilityMeasure"/>
        <rdfs:range rdf:resource="xsd:decimal"/>
    </rdf:Description>

</rdf:RDF>
